{
  "builds": [
    {
      "src": "main-website/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "editor/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/editor/(.*)",
      "dest": "/editor/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/main-website/$1"
    }
  ]
}