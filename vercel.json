{
  "version": 2,
  "builds": [
    { "src": "google-docs/package.json", "use": "@vercel/next" },
    { "src": "studio-draft-implementation/package.json", "use": "@vercel/next" }
  ],
  "routes": [
    { "src": "/editor/(.*)", "dest": "/google-docs/$1" },
    { "src": "/(.*)", "dest": "/studio-draft-implementation/$1" }
  ]
}