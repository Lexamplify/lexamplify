{
    "rewrites": [
      {
        "source": "/editor/(.*)",
        "destination": "/editor/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/main-website/$1"
      }
    ],
    "builds": [
      {
        "src": "main-website/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "editor/package.json",
        "use": "@vercel/node"
      }
    ]
  }