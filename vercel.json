{
    "rewrites": [
      {
        "source": "/editor/(.*)",
        "destination": "/google-docs/$1"
      },
      {
        "source": "/(.*)",
        "destination": "/studio-draft-implementation/$1"
      }
    ],
    "builds": [
      {
        "src": "google-docs/package.json",
        "use": "@vercel/node"
      },
      {
        "src": "studio-draft-implementation/package.json",
        "use": "@vercel/node"
      }
    ]
  }